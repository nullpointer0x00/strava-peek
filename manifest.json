{
    "manifest_version" : 2, 
    "name" : "Strava Peek",
    "options_page" : "options.html",
    "description" : "Peek at my feed.",
    "version" : "1.0", 
    "browser_action" : {
	"default_icon" : "icon.png",
	"default_popup" : "popup.html"
    },
    "permissions" : [
	"activeTab", 
	"storage",
	"http://ajax.googleapis.com/",
	"https://www.strava.com/",
	"*://*/"
    ]
}
